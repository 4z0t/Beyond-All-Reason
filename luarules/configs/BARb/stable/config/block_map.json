// Mono-space font required
{
"terrain": {
	"analyze": "armada_commander",

	// These constants control how to decide between Seas and Lakes.
	"lake_tiles": 100,  // 300,
	"lake_width_tiles": 10,  // 32,

	// At least area_min_tiles connected Tiles are necessary for an Area to be created.
	"area_min_tiles": 32,  // 64,

	"areas_merge_size": 64,  // 80,
	"areas_merge_altitude": 64  // 80  // 10 * altitude_scale
},

"building": {
	"class_land": {
		"fac_land": {
			// "type": [<blocker_shape>, <structure_type>]
			// Available blocker_shape: rectangle, circle.
			// Available structure_type: factory, mex, geo, pylon, convert, engy_low, engy_mid, engy_high, def_low, def_mid, def_high, special, nano, terra, unknown
			"type": ["rectangle", "factory"],

			// Unit of measurement: 1 size/yard/radius = SQUARE_SIZE * 2 = 16 elmos, integer.
			// Offset in South facing
			"offset": [0, 7],  // default: [0, 0]

			// Size of a blocker without yard
//			"size": [7, 7],  // default: size of a unit

			// Spacer, blocker_size = size + yard
			"yard": [8, 20],  // default: [0, 0]

			// Integer radius of a blocker or description string.
			// Available string values: explosion, expl_ally = [radius ~ 1 player .. radius/2 ~ 4+ players]
//			"radius": "expl_ally",  // default: "explosion"

			// "ignore": [<structure_type>, <structure_type>, ...]
			// Ignore specified structures.
			// Additional values: none, all
			"ignore": ["def_low"]  // default: ["none"]
//			"not_ignore": ["all"]  // default: ["all"]
		},
		"fac_land2": {
			"type": ["rectangle", "factory"],
			"offset": [0, 4],
			"yard": [-2, 42]
		},
		"fac_air": {
			"type": ["rectangle", "factory"],
			"yard": [8, 8]
		},
		"fac_water": {
			"type": ["rectangle", "factory"],
			"offset": [0, 4],
			"yard": [10, 12]
		},
		"fac_strider": {
			"type": ["rectangle", "special"],
			"offset": [0, 12],
			"yard": [16, 20]
		},
		"solar": {
			"type": ["rectangle", "engy_low"],
 			"ignore": ["engy_low", "def_low", "nano"]
		},
		"wind": {
			"type": ["circle", "engy_low"],
			"radius": 4,  // default: "explosion"
			"not_ignore": ["factory", "engy_low", "convert", "terra"]
		},
		"geo": {
			"type": ["rectangle", "geo"],
			"yard": [2, 2],
			"ignore": ["all"]
		},
		"geo2": {
			"type": ["rectangle", "geo"],
			"ignore": ["all"]
		},
		"fusion": {
			"type": ["circle", "engy_mid"],
			"radius": 6,
			"ignore": ["mex", "engy_mid", "def_low"]
		},
		"singu": {
			"type": ["circle", "engy_high"],
			"radius": 8,
			"ignore": ["mex", "engy_high", "def_low"]
		},
		"pylon": {
			"type": ["circle", "pylon"],
			"not_ignore": ["factory", "pylon", "terra"]  // default: ["all"]
		},
		"store": {
			"type": ["rectangle", "mex"],
			"not_ignore": ["factory", "terra"]
		},
		"mex": {
			"type": ["rectangle", "mex"],
//			"offset": [0, -5],
			"yard": [2, 2],
			"ignore": ["all"]
		},
		"mex2": {
			"type": ["rectangle", "mex"],
			"ignore": ["all"]
		},
		"converter": {
			"type": ["rectangle", "convert"],
			"yard": [8, 8],
			"ignore": ["convert"]
		},
		"def_low": {
			"type": ["circle", "def_low"],
			"radius": 5,  // 80 / (SQUARE_SIZE * 2)
			"ignore": ["engy_mid", "engy_high", "engy_low", "nano"]
		},
		"caretaker": {
			"type": ["rectangle", "nano"],
			"ignore": ["mex", "def_low", "engy_mid", "engy_high"]
		},
		"superweapon": {
			"type": ["circle", "special"],
			"ignore": ["mex", "def_low", "engy_high"]
		},
		"protector": {
			"type": ["circle", "special"],
			"ignore": ["mex", "def_low", "engy_mid", "engy_high"]
		},
//		"terraform": {
//			"type": ["rectangle", "special"],
//			"size": [7, 7]  // int2(3 + 4, 3 + 4)
//		},
		"strider": {
			"type": ["rectangle", "special"],
			"yard": [4, 4],
			"ignore": ["all"]
		},
		"small": {
			"type": ["rectangle", "unknown"],
			"yard": [4, 4],
			"not_ignore": ["factory", "def_low", "terra"]
		},
		"wall": {
			"type": ["rectangle", "def_low"],
			"not_ignore": ["factory", "mex", "geo", "terra"]
		},
		"_default_": {
			"type": ["rectangle", "unknown"],
			"yard": [6, 6],
			"ignore": ["engy_high"]
		}
	},
	// Water overrides land. Map considered as water if amount of land < 40%
	"class_water" : {
		"wind": {
			"type": ["rectangle", "engy_low"],
			"ignore": ["engy_low"]
		}
	},
	"instance": {
		"fac_land": ["armada_botlab", "armada_advancedbotlab", "armada_vehicleplant", "armada_advancedvehicleplant", "coralab", "corvp", "coravp"],
		"fac_land2": ["corlab", "armada_hovercraftplatform", "armada_navalhovercraftplatform", "corhp", "corfhp"],
		"fac_air": ["armada_aircraftplant", "corap", "armada_advancedaircraftplant", "coraap"],
		"fac_water": ["armada_shipyard", "armada_advancedshipyard", "armada_amphibiouscomplex", "armada_seaplaneplatform", "corsy", "corasy", "coramsub", "corplat"],
		"fac_strider": ["armada_experimentalgantry", "armada_experimentalgantryuw", "corgant", "corgantuw"],
		"solar": ["armada_solarcollector", "corsolar"],
		"wind": ["armada_windturbine", "corwin", "armada_tidalgenerator", "cortide"],
		"geo": ["armada_geothermalpowerplant", "corgeo"],
		"geo2": ["armada_advancedgeothermalpowerplant", "corageo"],
		"fusion": ["armada_advancedsolarcollector", "coradvsol"],
		"singu": ["armada_fusionreactor", "armada_navalfusionreactor", "armada_cloakablefusionreactor", "armada_advancedfusionreactor", "corfus", "coruwfus", "corafus"],
		"store": ["armada_metalstorage", "armada_energystorage", "cormstor", "corestor"],
		"mex": ["armada_metalextractor", "cormex", "armada_navalmetalextractor", "coruwmex"],
		"mex2": ["armada_advancedmetalextractor", "cormoho", "armada_navaladvancedmetalextractor", "coruwmme"],
		"converter": ["armada_energyconverter", "cormakr", "armada_advancedenergyconverter", "cormmkr", "armada_navalenergyconverter", "corfmkr", "armada_navaladvancedenergyconverter", "coruwmmm"],
		"def_low": ["armada_sentry", "armada_dragonsclaw", "corllt", "cormaw"],
		"caretaker": ["armada_constructionturret", "armada_constructionturretplat", "armasp", "cornanotc", "cornanotcplat", "corasp"],
		"small": ["armada_radartower", "corrad", "armada_navalradar", "corfrad"],
		"wall": ["armada_dragonsteeth", "cordrag"]
	}
}
}
